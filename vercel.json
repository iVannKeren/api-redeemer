{
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/shop$", "dest": "/shop.html" },
    { "src": "^/clientarea$", "dest": "/clientarea.html" },
    { "src": "^/register$", "dest": "/register.html" },
    { "src": "^/products$", "dest": "/products.html" },
    { "src": "^/payment$", "dest": "/payment.html" },

    { "src": "^/api/(.*)$", "dest": "/api/index.js" }
  ]
}